        <TabPane layoutX="0.0" layoutY="0.0" prefHeight="400.0" prefWidth="600.0" rotateGraphic="true" side="BOTTOM" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="home" text="Home">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Label layoutX="42.0" layoutY="21.0" prefHeight="17.0" prefWidth="366.0" text="Label" />
                                <Button layoutX="456.0" layoutY="16.0" mnemonicParsing="false" text="Button" />
