                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="orders" text="Orders">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TableView fx:id="oTable" prefHeight="367.0" prefWidth="600.0">
                                    <columns>
                                        <TableColumn fx:id="cTime" prefWidth="75.0" text="Order create time" />
                                        <TableColumn fx:id="cPrise" prefWidth="75.0" text="Prise" />
                                        <TableColumn fx:id="cBrand" prefWidth="75.0" text="Brand" />
                                        <TableColumn fx:id="cStatus" prefWidth="75.0" text="Status" />
                                        <TableColumn fx:id="cType" prefWidth="75.0" text="Vehicle type" />
                                    </columns>
                                    <columnResizePolicy>
                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                    </columnResizePolicy>
                                </TableView>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="account" text="Account">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ListView prefHeight="369.0" prefWidth="600.0" />
                     </children></AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="about" text="About">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
